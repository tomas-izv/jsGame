{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "rootDir": "server/src",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": [
        "server/src/**/*"
    ],
    "exclude": [
        "server/node_modules",
        "dist"
    ]
}